{
  "trailingComma": "all",
  "overrides": [
    {
      "files": ["*.html", "*.ts"],
      "options": {
        "printWidth": 120
      }
    },
    {
      "files": ["./src/data/**/*.ts"],
      "options": {
        "singleQuote": false,
        "quoteProps": "preserve",
        "printWidth": 9999,
        "trailingComma": "none"
      }
    }
  ]
}
